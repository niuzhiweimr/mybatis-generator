<?xml version="1.0" encoding="UTF-8"?>
<daoj>
    <dataSource>
        <property name="jdbcDriver" value="com.mysql.jdbc.Driver"/>
        <property name="jdbcUrl" value="jdbc:mysql://10.10.1.21:3306/syhd_test"/>
        <property name="username" value="syhd"/>
        <property name="password" value="syhd!@#321"/>
    </dataSource>
    <params>
        <!-- 设定生成后的实体类所在的包 -->
        <property name="entityPackage" value="com.morp.model"/>
        <!-- 设定生成后的DAO接口所在的包 -->
        <property name="daoPackage" value="com.morp.dao"/>
