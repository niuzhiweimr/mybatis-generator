<?xml version="1.0" encoding="UTF-8"?>
<daoj>
    <dataSource>
        <property name="jdbcDriver" value="com.mysql.jdbc.Driver"/>
        <property name="jdbcUrl" value="jdbc:mysql://127.0.0.1:3306/kjplus"/>
        <property name="username" value="root"/>
        <property name="password" value="111111"/>
    </dataSource>
    <params>
        <!-- 设定生成后的实体类所在的包 -->
        <property name="entityPackage" value="com.morp.model"/>
        <!-- 设定生成后的DAO接口所在的包 -->
        <property name="daoPackage" value="com.morp.dao"/>
        <!-- 设定代码生成的输出目录, 支持相对路径或绝对路径 -->
        <property name="outputDirc" value="generated-files"/>        
        <!-- 列出要生成的表名, 多个表可以逗号(',')分隔。 注意，数据库表必须指定了主键列。 -->
        <property name="tables" value="t_user,t_info"/>
    </params>
</daoj>